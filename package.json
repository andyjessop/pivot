    "nx": "16.0.1",
    "postcss-nesting": "^11.2.1",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.0",
    "vite": "^4.2.0",
    "vitest": "^0.29.7",
    "nx-cloud": "16.0.5",
    "@nx/devkit": "16.0.1",
    "@nx/workspace": "16.0.1",
    "@nx/eslint-plugin": "16.0.1",
    "@nx/js": "16.0.1"
  },
  "engines": {
    "node": "18.13.0"
